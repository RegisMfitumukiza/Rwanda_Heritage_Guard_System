# API Configuration
VITE_API_URL=http://localhost:8080/api

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id

# Feature Flags
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_DARK_MODE=true

# External Services
VITE_MAP_API_KEY=your-map-api-key
VITE_STORAGE_BUCKET_URL=your-storage-bucket-url

# Analytics (if needed)
VITE_ANALYTICS_ID=your-analytics-id

# Cache Configuration
VITE_CACHE_DURATION=2500
VITE_MAX_CACHE_ITEMS=200

# Image Configuration
VITE_MAX_IMAGE_SIZE=5242880
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/gif

# Application Configuration
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=Rwanda Heritage Guard
VITE_APP_DESCRIPTION=Preserving and promoting Rwanda's rich cultural heritage






